{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/backend/core/wsgi.py"
        },
        {
            "source": "/admin/:path*",
            "destination": "/backend/core/wsgi.py"
        },
        {
            "source": "/static/:path*",
            "destination": "/backend/core/wsgi.py"
        }
    ]
}
